#include <bits/stdc++.h>
using namespace std;
int main() {
    int n;
    cin>>n;
    if(n==1){
        cout<<1<<"\n";
    }
    else if(n<=3) cout<<"NO SOLUTION\n";
    else if(n==4) cout<<"2 4 1 3\n";
    else{
        int initial=1;
        int num=initial;
        for(int i=0;i<n;i+=1){
            cout<<num<<" ";
            num+=2;
            if(num>n){
                initial+=1;
                num=initial;
            }
        }
    }
    return 0;
}
