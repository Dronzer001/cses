#include <bits/stdc++.h>
using namespace std;
int main() {
    int n;
    cin>>n;
    vector<int>arr(n-1,0);
    for(int i=0;i<n-1;i+=1) cin>>arr[i];
    sort(arr.begin(),arr.end());
    int k=1;
    for(int i=0;i<n-1;i+=1){
        if(k!=arr[i]) break;
        k+=1;
    }
    cout<<k<<"\n";
    return 0;
}
